//concatenate the array taken in as a parameter to a new empty array.
//Then Use the sort() method and create a function to sort the new array in ascending order.
//Avoiding mutation of original array is the goal

var globalArray = [5, 6, 3, 2, 9];
function nonMutatingSort(arr) {
  // Add your code below this line
  return [].concat(arr).sort(function(a, b) {
    return a - b;
  });
  // Add your code above this line
}
nonMutatingSort(globalArray);
